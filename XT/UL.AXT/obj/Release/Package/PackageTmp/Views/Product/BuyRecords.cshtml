@model List<Model.UserOrderDTO>
@{
    ViewBag.Title = "BuyRecords";
    Layout = "~/Views/Shared/Main.cshtml";
}

<div class="mainM">
    <div class="detail grayC_border white_bg">
        <div class="detailT">
            <a class="oh"><span class="fl lh20 f14">抢购记录（@Model.Count）</span></a>
        </div>
        <div class="detailM">
            <div class="record">
                <ul>
                    @foreach (var order in Model)
                    {
                        <li>
                            <img src="@order.PhotoPath" class="fl" />
                            <p class="oh"><span class="fl">@order.UserName</span><span class="fr">  @order.City</span></p>
                            <p class="oh"><span class="fl gray9">@order.CreateTime</span><span class="fr grayC"> </span></p>
                        </li>
                    }
                   
                </ul>
            </div>
        </div>
    </div>

</div>


